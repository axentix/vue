{
  "name": "@axentix/vue",
  "version": "0.1.1",
  "description": "Vue Axentix is the Axentix integration for the VueJS framework.",
  "license": "MIT",
  "author": "Axel SIMONET, Vincent LEVEQUE",
  "main": "dist/vue2/vue-axentix.cjs.js",
  "module": "dist/vue2/vue-axentix.esm.js",
  "browser": "dist/vue2/vue-axentix.esm.js",
  "unpkg": "dist/vue2/vue-axentix.min.js",
  "scripts": {
    "test": "jest",
    "test:u": "jest -u",
    "serve": "vue-cli-service serve ./dev/main.js",
    "serve:storybook": "start-storybook -p 6006",
    "build": "npm run build:2 && npm run build:3",
    "build:cjs": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:esm": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format esm",
    "build:iife": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:2": "npm run switch:2 && cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:3": "npm run switch:3 && cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:storybook": "build-storybook",
    "lint": "vue-cli-service lint",
    "switch:2": "vue-demi-switch 2 && npm i -D rollup-plugin-vue@5.1.9",
    "switch:3": "vue-demi-switch 3 && npm i -D rollup-plugin-vue@6.0.0",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:axentix/vue.git"
  },
  "bugs": {
    "url": "https://github.com/axentix/vue/issues"
  },
  "keywords": [
    "axentix",
    "framework",
    "front-end",
    "responsive",
    "css",
    "js",
    "sass",
    "web",
    "vuejs",
    "vue",
    "library",
    "vue-extension"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README",
    "CHANGELOG"
  ],
  "dependencies": {
    "axentix": "^1.2.1",
    "vue-demi": "^0.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/vue": "^6.2.9",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/composition-api": "^1.0.0-rc.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@whitespace/storybook-addon-html": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "jest-vue-preprocessor": "^1.7.1",
    "minimist": "^1.2.5",
    "prettier": "^2.3.0",
    "rollup": "^2.47.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.32.12",
    "sass-loader": "^8.0.2",
    "storybook-addon-pseudo-states": "^1.0.0",
    "storybook-dark-mode": "^1.0.8",
    "typescript": "^4.2.4",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.12",
    "vue3": "npm:vue@3"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.9",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
