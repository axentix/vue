{
  "name": "@axentix/vue",
  "version": "0.11.6",
  "description": "Vue Axentix is the Axentix integration for the VueJS framework.",
  "license": "MIT",
  "author": "Axel SIMONET, Vincent LEVEQUE",
  "main": "dist/vue2/vue-axentix.cjs.js",
  "module": "dist/vue2/vue-axentix.es.js",
  "browser": "dist/vue2/vue-axentix.es.js",
  "unpkg": "dist/vue2/vue-axentix.iife.js",
  "scripts": {
    "dev": "npm run switch:2 && vite --host",
    "build": "npm run build:2 && npm run build:3",
    "build:2": "npm run switch:2 && vite build",
    "build:3": "npm run switch:3 && vite build",
    "lint": "eslint src --fix",
    "switch:2": "vue-demi-switch 2",
    "switch:3": "vue-demi-switch 3 vue3",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:axentix/vue.git"
  },
  "bugs": {
    "url": "https://github.com/axentix/vue/issues"
  },
  "keywords": [
    "axentix",
    "framework",
    "front-end",
    "responsive",
    "css",
    "js",
    "sass",
    "web",
    "vuejs",
    "vue",
    "library",
    "vue-extension"
  ],
  "files": [
    "dist",
    "scripts",
    "LICENSE",
    "README",
    "CHANGELOG"
  ],
  "dependencies": {
    "axentix": "^2.0.0-beta.5",
    "vue-demi": "^0.12.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.30",
    "@vue/composition-api": "^1.4.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^6.2.2",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "typescript": "^4.5.5",
    "vite": "^2.8.1",
    "vite-plugin-vue2": "^1.9.3",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "vue3": "npm:vue@^3.2.30"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.9",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
